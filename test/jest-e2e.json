{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "cache": false,
  "watchman": false,
  "cacheDirectory": "./.tmp",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  }
}
